#include<stdio.h>
void main()
{
	int n,i,flag=0,x,count=0,a[20],time=3,space=7;
	printf("Enter the number of array elements: ");
	time++;
	scanf("%d",&n);
	time++;
	printf("Enter the array elements: \n");
	time++;
	for(i=0;i<n;i++)
	{time++;
		scanf("%d",&a[i]);
	}
	printf("Enter the value to be searched: ");
	time++;
	scanf("%d",&x);
	time++;
	for(i=0;i<n;i++)
	{time++;
		if(a[i]==x)
		{time++;
			count++;
			time++;
			flag=1;
			time++;
		}
	}
	if(flag==1)
	{time++;
		if(count>=1)
		{time++;
			printf("Element is found %d times",count);
			time++;
		}
	}
	else
	{time++;
		printf("Element not found");
	}
	space=(space*4)+(n*4);
	printf("\n time complexity= %d",time);
	printf("\n space complexity= %d",space);
}
			
	
