#include <stdio.h>
 void main ()
 {
     int n,i,j,a[50],small,c,temp;
     c++;
     printf("Enter the size of the array:\n ");
     c++;
     scanf("%d",&n);
     printf("Enter the elements :\n");
     c++;
     for(i=0;i<n;i++)
     {
            c++;
         scanf("%d",&a[i]);
         c++;
     }
     for(i=0;i<n;i++)
     {
         c++;
         small=i;
         for( j=i+1;j<n;j++)
         {
             c++;
             if(a[small]>a[j])
             {
                 c++;
                 small=j;
             }
         }
         if(small!=i)31 {
             temp = a[i]; c++;
             a[i]=a[small]; c++;
             a[small]=temp; c++;
         }
     }
     printf( "The sorted elements is :\n"); c++;
     for(i=0;i<n;i++)
     {
         c++;
         printf("%d \t",a[i]); c++;
     }
     c++;
     printf("The space complexity = %d \n" , (4*n+32));
     c++;
     printf("The time complexity = %d" ,c );
 }
